    "prime= []\n",
    "limit = int(input(\"Please enter the limit for prime numbers :\"))\n",
    "for number in range (2, limit+1):\n",
    "    status=True\n",
    "    for i in range(2, number):\n",
    "        if number % i == 0 : status = False\n",
    "    if status:\n",
    "        prime.append(number)\n",
    "print(prime)"
